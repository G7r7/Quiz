{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && cp docker-compose.prod.yml dist/docker-compose.yml && cp fullchain.pem dist/fullchain.pem && cp privkey.pem dist/privkey.pem && cp nginx.conf dist/nginx.conf",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,.vue --ignore-path .gitignore --fix src",
    "format": "prettier .  --write",
    "generate-client": "npx openapi-typescript-codegen --input http://localhost:5000/openapi.json --output ./src/providers --client axios"
  },
  "dependencies": {
    "@mdi/font": "^6.7.96",
    "axios": "^0.27.2",
    "pinia": "^2.0.14",
    "socket.io-client": "^4.5.0",
    "vue": "^3.2.25",
    "vue-router": "^4.0.15",
    "vuetify": "3.0.0-beta.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "eslint": "^8.15.0",
    "eslint-plugin-vue": "^8.7.1",
    "openapi-typescript": "^5.3.0",
    "prettier": "2.6.2",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vue-tsc": "^0.34.7"
  }
}
